{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "func-names": [
      "error",
      "never"
    ],
    "import/no-extraneous-dependencies": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "import/first": "off",
    "react/prefer-stateless-function": "off",
    "linebreak-style": "off",
    "max-len": "off",
    "react/no-danger": "off"
  }
}
